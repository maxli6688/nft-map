{
  "name": "kvm-tile-map-workspace",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "build:lib": "pnpm --filter @kvm/kvm-tile-map build",
    "build:app": "pnpm --filter kvm-map-app build",
    "dev": "pnpm -r --parallel dev",
    "dev:lib": "pnpm --filter @kvm/kvm-tile-map dev",
    "dev:app": "pnpm --filter kvm-map-app dev",
    "dev:all": "pnpm --stream -r dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "serve": "pnpm -r --parallel serve"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/node": "^18.19.130",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.7.0",
    "less": "^4.4.2",
    "typescript": "^5.9.3",
    "vite": "^6.3.6"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
